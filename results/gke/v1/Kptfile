  type: oci
  oci:
    image: us-docker.pkg.dev/dev-fleet-9045b/my-blueprints/gke:v1
    digest: us-docker.pkg.dev/dev-fleet-9045b/my-blueprints/gke@sha256:33901be03c6b557c5cff9744a8fe2e5c9784b6bc2daf946a2b151a6845f7be66
